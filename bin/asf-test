echo "----------------------------N-Modular Redundancy test (Tick Milestone)----------------------------"
echo "3 modules, 0.5 probability of fault"
time ./maskD  --a3 p=4 b=100 ../tests/stochastic/nominal/nmr/nmr[].mdp ../tests/stochastic/faulty/nmr/nmr[3-0.5].mdp
echo "5 modules, 0.5 probability of fault"
