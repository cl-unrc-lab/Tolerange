#script for replicating experimental evaluation results

echo "----------------------------Memory Cell test (Refresh Milestone)----------------------------"

echo "3 bits, 0.5 probability of fault, 0.5 probability of refresh"
time ./Tolerange    b=30000 ../tests/stochastic/nominal/memory/memoryRefreshMilestone[0.5].mdp ../tests/stochastic/faulty/memory/memoryRefreshMilestone[3-0.5-0.5].mdp

echo "5 bits, 0.5 probability of fault, 0.5 probability of refresh"
time ./Tolerange    b=30000 ../tests/stochastic/nominal/memory/memoryRefreshMilestone[0.5].mdp ../tests/stochastic/faulty/memory/memoryRefreshMilestone[5-0.5-0.5].mdp

echo "7 bits, 0.5 probability of fault, 0.5 probability of refresh"
time ./Tolerange    b=30000 ../tests/stochastic/nominal/memory/memoryRefreshMilestone[0.5].mdp ../tests/stochastic/faulty/memory/memoryRefreshMilestone[7-0.5-0.5].mdp

