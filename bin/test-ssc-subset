echo "----------------------------N-Modular Redundancy: N Processor, N Voter test (Tick Milestone)----------------------------"
echo "N=3, 0.05 probability of processor fault, 0.05 probability of voter fault"
time ./Tolerange     b=1000 ../tests/stochastic/nominal/nmr-proc-mem/nmr-proc-mem[].pts ../tests/stochastic/faulty/nmr-proc-mem/nmr-proc-mem[3-0.05-0.05].pts
